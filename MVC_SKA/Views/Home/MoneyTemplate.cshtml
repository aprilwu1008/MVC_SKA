@using MVC_SKA.Models
@model MoneyPartialViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            @{
                foreach (var money in @Model.List)
                {
                    <tr>
                        <td>@money.Seq</td>
                        <td>@money.MoneyType</td>
                        <td>@money.DateTime</td>
                        <td>@money.Count</td>
                    </tr>
                }
            }
        </table>
    </div>
</div>